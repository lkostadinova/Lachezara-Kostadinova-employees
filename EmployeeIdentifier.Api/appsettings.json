{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.DataProtection": "Error",
      "Microsoft.IdentityModel.LoggingExtensions": "Critical",
      "Microsoft.EntityFrameworkCore.Database.Command": "None"
    }
  },
  "AllowedHosts": "*",
  "ErrorHandlingSettings": {
    "ShowException": true
  },
  "MetricsEnabled": false,
  "MetricsPort": 5002,
  "MetricsPrefixApi": "employeeidentifier_api",
  "Environment": "Local"
}
